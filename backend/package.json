{
  "name": "credentials-provider",
  "version": "1.0.0",
  "description": "Node.js based credentials-provider",
  "main": "index.ts",
  "scripts": {
    "start" : "nodemon index.js",
    "db:generate": "drizzle-kit generate:pg --config=./drizzle.config.ts",
    "db:delete": "drizzle-kit drop --config=./drizzle.config.ts",
    "db:push": "drizzle-kit push:pg --config=./drizzle.config.ts",
    "db:deploy": "ts-node ./drizzle/migrations/migrate.ts"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/K-Link-Tech/credentials-provider.git/backend"
  },
  "keywords": [
    "CR-B"
  ],
  "author": "Aung Tay Zar Maung",
  "license": "ISC",
  "dependencies": {
    "commitizen": "^4.3.0",
    "drizzle-orm": "^0.27.2",
    "express": "^4.18.2",
    "postgres": "^3.3.5",
    "uuid": "^9.0.0"
  },
  "devDependencies": {
    "cz-emoji-conventional-changelog": "^0.0.0",
    "drizzle-kit": "^0.19.9",
    "nodemon": "^3.0.1",
    "typescript": "^5.1.3"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-emoji-conventional-changelog"
    }
  }
}
